@using Microsoft.AspNetCore.Http

@{
    Layout = "_Layout";
}

@inject IHttpContextAccessor HttpContextAccessor

 @{ 
     var sesskey = HttpContextAccessor.HttpContext.User.Claims.FirstOrDefault(x => x.Type == "SessionKey");
     var HasRestaurantConfigured = HttpContextAccessor.HttpContext.User.Claims.FirstOrDefault(x => x.Type == "HasRestaurantConfigured").Value;
  }

@if (HasRestaurantConfigured == "true")
{
    @Html.Partial("UserDashboard.cshtml");
}
else
{
    @Html.Partial("RestaurantConfiguration.cshtml");
}
